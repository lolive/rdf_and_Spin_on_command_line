# baseURI: http://example.org/unnamed
# imports: http://spinrdf.org/spin

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix spl: <http://spinrdf.org/spl#> .
@prefix unnamed: <http://example.org/unnamed#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix edm: <http://www.europeana.eu/schemas/edm/> .

<http://example.org/unnamed>
  rdf:type owl:Ontology ;
  owl:imports <http://spinrdf.org/spin> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
unnamed:CHO
  rdf:type owl:Class ;
  spin:constraint
  [ a       sp:Construct ;
    sp:text """
               CONSTRUCT {
                           _:violation a                  spin:ConstraintViolation ;
                                       spin:violationRoot ?this ;
                                       spin:violationPath ?this ;
                                       rdfs:label         "edm:isShownAt must have no trailing whitespace"
               } WHERE { 
                           select ?this ?o1 ?o2 where {
                                                          ?this rdf:type unnamed:CHO .
                                                          ?this edm:isShownAt ?o1 .
                                                          FILTER ( regex(?o1,"((^ +.*$)|(^.* +$))" ))       
                           }
               }
            """
  ] .
